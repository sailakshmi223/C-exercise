#include <stdio.h>

int main()
{
    char s[100];
    scanf("%s",s);
    int i;
    int countv=0;
    int countc=0;
    for(i=0;s[i]!='\0';i++){
      if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'){
        countv++;  
      }
      else{
        countc++;  
      }
    }
    printf("no of vowels:%d",countv);
    printf("\n");
    printf("no of consonents:%d",countc);
    

    return 0;
}
